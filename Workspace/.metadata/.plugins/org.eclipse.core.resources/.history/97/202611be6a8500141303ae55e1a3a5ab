package com.example.organizer;

import java.io.File;
import java.io.PrintWriter;
import java.util.List;

public class Zapis {
	File file;
	List<String> listaZadan = null;

	public Zapis(File file) {
		this.file = file;
	}

	public void ZapisPliku(String zadanie) {
		PrintWriter zapis = new PrintWriter(file.toString());
	    zapis.println("Ala ma kota, a kot ma Alê");
	    zapis.close();
	}
}
